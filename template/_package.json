{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "author": "<%= author %>",
  "private": true,
  "scripts": {
    <%_ if (server === 'none') { _%>
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    <%_ } else if (server === 'adonis') { _%>
    "serve:dev": "<%= pmRun %> dev",
    "dev": "nodemon --watch app --watch bootstrap --watch config --watch .env -x node server.js",
    "build": "cross-env NODE_ENV=production node ./ace nuxtbuild",
    "start": "cross-env NODE_ENV=production node server.js",
    <%_ } else { _%>
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    <%_ } _%>
    <%_ if (ui === 'framevuerk') { _%>
    "build:framevuerk": "framevuerk-builder -c ./framevuerk-config.js",
    "prepublish": "<%= pmRun %> build:framevuerk",
    <%_ } _%>
    <%_ if (eslint) { _%>
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    <%_ } _%>
    <%_ if (test !== 'none') { _%>
    "test": "<%= test %>",
    <%_ } _%>
    "": ""
  },
  <%_ if (lintStaged) { _%>
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  <%_ } _%>
  "dependencies": {
    <%_ if (edge) { _%>
    "nuxt-edge": "latest",
    <%_ } else { _%>
    "nuxt": "^2.0.0",
    <%_ } _%>
    <%_ if (server !== 'none') { _%>
    "cross-env": "^5.2.0",
    <%_ } _%>
    <%_ if (server === 'express') { _%>
    "express": "^4.16.4",
    <%_ } else if (server === 'koa') { _%>
    "koa": "^2.6.2",
    <%_ } else if (server === 'hapi') { _%>
    "@hapi/hapi": "^18.3.1",
    "@nuxtjs/hapi": "^2.2.1",
    <%_ } else if (server === 'micro') { _%>
    "micro": "^9.3.3",
    "micro-route": "^2.5.0",
    <%_ } else if (server === 'fastify') { _%>
    "fastify": "^1.13.3",
    <%_ } else if (server === 'feathers') { _%>
    "@feathersjs/feathers": "^3.3.1",
    "@feathersjs/express": "^1.3.1",
    "@feathersjs/configuration": "^2.0.6",
    <%_ } else if (server === 'adonis') { _%>
    "@adonisjs/ace": "^4.0.7",
    "@adonisjs/auth": "^2.0.10",
    "@adonisjs/bodyparser": "^1.0.8",
    "@adonisjs/cors": "^1.0.2",
    "@adonisjs/fold": "^4.0.5",
    "@adonisjs/framework": "^4.0.27",
    "@adonisjs/ignitor": "^1.0.14",
    "@adonisjs/lucid": "^4.0.22",
    "@adonisjs/session": "^1.0.19",
    "@adonisjs/shield": "^1.0.4",
    <%_ } _%>
    <%_ if (ui === 'bootstrap') { _%>
    "bootstrap-vue": "^2.0.0-rc.11",
    "bootstrap": "^4.1.3",
    <%_ } else if (ui === 'bulma') { _%>
    "@nuxtjs/bulma": "^1.2.1",
    <%_ } else if (ui === 'element-ui') { _%>
    "element-ui": "^2.4.11",
    <%_ } else if (ui === 'ant-design-vue') { _%>
    "ant-design-vue": "^1.1.10",
    <%_ } else if (ui === 'buefy') { _%>
    "nuxt-buefy": "^0.3.2",
    <%_ } else if (ui === 'iview') { _%>
    "iview": "^3.1.5",
    <%_ } else if (ui === 'framevuerk') { _%>
    "framevuerk": "^2.2.5",
    <%_ } _%>
    <%_ if (axios) { _%>
    "@nuxtjs/axios": "^5.3.6",
    <%_ } _%>
    <%_ if (pwa) { _%>
    "@nuxtjs/pwa": "^3.0.0-0",
    <%_ } _%>
    <%_ if (ui === 'tachyons') { _%>
    "tachyons": "^4.11.1",
    <%_ } _%>
    "": ""
  },
  "devDependencies": {
    <%_ if (server !== 'none') { _%>
    "nodemon": "^1.18.9",
    <%_ } _%>
    <%_ if (ui === 'tailwind') { _%>
    "@nuxtjs/tailwindcss": "^1.0.0",
    <%_ } else if (ui === 'vuetify') { _%>
    "@nuxtjs/vuetify": "^1.0.0",
    <%_ } else if (ui === 'framevuerk') { _%>
    "framevuerk-builder": "^2.0.2",
    <%_ } _%>
    <%_ if (eslint) { _%>
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    <%_ } _%>
    <%_ if (prettier) { _%>
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "prettier": "^1.16.4",
    <%_ } _%>
    <%_ if (lintStaged) { _%>
    "husky": "^2.6.0",
    "lint-staged": "^8.2.1",
    <%_ } _%>
    <%_ if (test !== 'none') { _%>
    "@vue/test-utils": "^1.0.0-beta.27",
    <%_ } _%>
    <%_ if (test === 'jest') { _%>
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "vue-jest": "^4.0.0-0",
    <%_ } else if (test === 'ava') { _%>
    "ava": "^2.2.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-env": "^3.2.5",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0",
    "require-extension-hooks-vue": "^2.0.0",
    <%_ } _%>
    "": ""

  }
}
